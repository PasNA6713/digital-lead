KANA,mBAMA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA;AARA;AAfA,CAAA","sourcesContent":["<template>\r\n  <div id=\"login\">\r\n    <v-app id=\"sublogin\">\r\n\r\n      <h2>Вход в личный кабинет</h2>\r\n      <v-form\r\n        ref=\"form\"\r\n        v-model=\"valid\"\r\n        lazy-validation\r\n      >\r\n    \r\n        <v-text-field\r\n          v-model=\"email\"\r\n          :rules=\"emailRules\"\r\n          label=\"E-mail\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-text-field\r\n          v-model=\"password\"\r\n          :rules=\"passwordRules\"\r\n          label=\"Password\"\r\n          type=\"password\"\r\n          required\r\n        ></v-text-field>\r\n\r\n        <v-btn\r\n          :disabled=\"!valid\"\r\n          color=\"primary\"\r\n          class=\"mr-4\"\r\n          @click=\"validate\"\r\n        >\r\n          Войти\r\n        </v-btn>\r\n    \r\n        <v-btn\r\n          color=\"error\"\r\n          class=\"mr-4\"\r\n          @click=\"reset\"\r\n        >\r\n          Очистить\r\n        </v-btn>\r\n      </v-form>\r\n  </v-app>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n  export default {\r\n    data: () => ({\r\n      valid: true,\r\n      password: '',\r\n      passwordRules: [\r\n        v => !!v || 'Пароль не может быть пустым',\r\n        v => (v && v.length > 10) || `Пароль должен быть длиннее 10 символов, а сейчас ${v.length}`,\r\n      ],\r\n      email: '',\r\n      emailRules: [\r\n        v => !!v || 'E-mail не может быть пустым',\r\n        v => /.+@.+\\..+/.test(v) || 'Введите корректный e-mail',\r\n      ]\r\n    }),\r\n\r\n    methods: {\r\n      validate () {\r\n        if (this.$refs.form.validate()){\r\n          this.$router.push('/')\r\n        }\r\n      },\r\n      reset () {\r\n        this.$refs.form.reset()\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  #login{\r\n    display: flex;\r\n    min-width: 1500px;\r\n    height: 725px;\r\n    margin: auto auto;\r\n   